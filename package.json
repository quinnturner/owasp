{
  "name": "owasp",
  "version": "1.2.0",
  "type": "module",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "author": {
    "name": "Quinn Turner"
  },
  "homepage": "https://github.com/quinnturner/owasp#readme",
  "bugs": {
    "url": "https://github.com/quinnturner/owasp/issues"
  },
  "keywords": ["owasp", "logging", "security"],
  "exports": {
    "./dom": {
      "import": {
        "types": "./dom/index.d.ts",
        "default": "./dom/index.js"
      },
      "require": {
        "types": "./dom/index.d.cts",
        "default": "./dom/index.cjs"
      }
    },
    "./vocab": {
      "import": {
        "types": "./vocab/index.d.ts",
        "default": "./vocab/index.js"
      },
      "require": {
        "types": "./vocab/index.d.cts",
        "default": "./vocab/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "files": ["dom", "vocab"],
  "scripts": {
    "format": "biome format ./src --write",
    "lint": "biome lint ./src",
    "check": "biome check --apply ./src",
    "build": "tsup --config tsup.config.ts"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.7.2",
    "@happy-dom/global-registrator": "^14.7.1",
    "bun-types": "^1.1.6",
    "tsup": "^8.0.2",
    "type-fest": "^4.18.1",
    "typescript": "^5.4.5"
  },
  "packageManager": "bun@1.1.0",
  "trustedDependencies": ["@biomejs/biome", "esbuild"]
}
